<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.tsoft.sign.mapper.ComMapper">

    <!-- 파일경로 저장 -->
    <insert id="insertFileUpload" parameterType="HashMap">
        insert into file
        ( FILE_TYPE
        <if test="CONTRACT_NO != null and CONTRACT_NO != ''">
            ,CONTRACT_NO
        </if>
        <if test="FORM_NM != null and FORM_NM != ''">
            ,FORM_NM
        </if>
        ,ORG_FILE_NM
        ,SAV_FILE_NM
        ,FILE_PATH
        ,REG_ID
        ,MOD_ID
        ) values (
        #{FILE_TYPE}
        <if test="CONTRACT_NO != null and CONTRACT_NO != ''">
            ,#{CONTRACT_NO}
        </if>
        <if test="FORM_NM != null and FORM_NM != ''">
            ,#{FORM_NM}
        </if>
        ,#{ORG_FILE_NM}
        ,#{SAV_FILE_NM}
        ,#{FILE_PATH}
        ,#{user}
        ,#{user}
        )
    </insert>

    <!-- 파일경로 저장 -->
    <insert id="insertFileUpload2" parameterType="kr.co.tsoft.sign.vo.admin.FormGridDto">
        insert into file
        ( FILE_TYPE
        <if test="contractNo != null and contractNo != ''">
            ,CONTRACT_NO
        </if>
        <if test="formNm != null and formNm != ''">
            ,FORM_NM
        </if>
        ,ORG_FILE_NM
        ,SAV_FILE_NM
        ,FILE_PATH
        ,REG_ID
        ,MOD_ID
        ) values (
        #{fileType}
        <if test="contractNo != null and contractNo != ''">
            ,#{contractNo}
        </if>
        <if test="formNm != null and formNm != ''">
            ,#{formNm}
        </if>
        ,#{orgFileNm}
        ,#{savFileNm}
        ,#{filePath}
        ,#{regId}
        ,#{modId}
        )
    </insert>
</mapper>
