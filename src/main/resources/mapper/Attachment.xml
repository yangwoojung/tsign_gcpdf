<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.tsoft.sign.mapper.admin.AttachmentMapper">

    <select id="selectAttachmentList" resultType="Map">
	/** [ADMIN].[AttachmentMapper.selectAttachmentList] */
	select 
		ATTACHMENT_SEQ
		,ATTACHMENT_CD
		,ATTACHMENT_NAME
		,OCR_YN
		,SCRAP_YN
		,PRIORITY
		,USE_YN
		,REG_DATE
		,REG_ID
		,MOD_DATE
		,MOD_ID
	from attachment	
	</select>

</mapper>
